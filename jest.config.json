{
  "testEnvironment": "jsdom",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "setupFilesAfterEnv": [
    "<rootDir>/jest.setup.ts"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "collectCoverageFrom": [
    "./src/**"
  ],
  "coverageThreshold": {
    "global": {
      "lines": 35
    }
  }
}